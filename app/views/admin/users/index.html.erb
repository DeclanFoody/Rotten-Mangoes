<h1> Admin Page </h1>
<%= paginate @users %>
<% @users.each do |user| %>

 <h3> <%= user.firstname %> <%= user.created_at %> (<%= link_to "edit", edit_admin_user_path(user) %>)(<%= link_to "delete", admin_user_path(user), method: :delete, data: {confirm: "Are you sure you want to delete this user?"}%>)</h3>
<% end %>
  